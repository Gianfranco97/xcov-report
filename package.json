    "conventional-github-releaser": "./node_modules/.bin/conventional-github-releaser",
    "github-releaser": "./node_modules/.bin/github-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TECLIB/fastlane-plugin-xcov_report"
  },
  "keywords": [
    "fastlane-plugin",
