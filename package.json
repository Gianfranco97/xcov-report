  },
  "license": "MIT",
  "social_media_url": "https://twitter.com/FlyveMDM",
  "bugs": {
    "url": "https://github.com/TECLIB/fastlane-plugin-xcov_report/issues"
  },
  "devDependencies": {
    "conventional-github-releaser": "^3.1.2",
    "node-github-release": "^0.2.1",
    "gh-pages": "^1.2.0",
    "standard-version": "^4.4.0"
  }
}
